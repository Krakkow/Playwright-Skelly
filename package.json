{
  "name": "playwright-skelly",
  "version": "1.0.0",
  "description": "skeleton for playwright automation framework",
  "scripts": {
    "prepare": "npx playwright install --with-deps",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "report": "playwright show-report",
    "clean:downloads": "node -e \"require('fs-extra').emptyDirSync('downloads')\""
