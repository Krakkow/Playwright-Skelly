    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "fs-extra": "^11.3.2",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "homepage": "https://github.com/Krakkow/Playwright-Skelly#readme",
